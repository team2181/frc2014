void Launcher::Reset() {
	while (limitLauncherDown->Get() == false) {
		speedControllerLauncher->Set(-1,0);
		Wait(0.005);
	}
	speedControllerLauncher->Set(0,0);
	Wait(0.005);
}
void Launcher::Launch() {
	while (limitLauncherUp->Get() == false) {
		speedControllerLauncher->Set(1,0);
		Wait(0.005);
	}
	speedControllerLauncher->Set(0,0);
	Wait(0.005);
}
void Launcher::Stop() {
	speedControllerLauncher->Set(0,0);
}
